{
    "testRegex": "/*.test.js$",
    "coveragePathIgnorePatterns": [
        "!<rootDir>/src/services/"
    ],
    "collectCoverage": true,
    "coverageReporters": ["text"],
    "coverageDirectory": "test-coverage",
    "coverageThreshold": {
     "global": {
     "branches": 0,
     "functions": 0,
     "lines": 0,
     "statements": 0
     }
    },
    "moduleDirectories": ["node_modules", "src"]
}